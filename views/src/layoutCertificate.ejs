<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <script src="/js/jquery-3.5.1.min.js"></script>

    <title>Certificado</title>
</head>
<style>

    @font-face {
    font-family: myFirstFont;
        src: url('/certificates/fonts/GraphikRegular_0.otf') format("opentype");
    }

</style>
<body>
    

</body>
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.6/moment.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment-timezone/0.5.23/moment-timezone-with-data-2012-2022.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.5.3/jspdf.min.js"></script>
<script src="/js/FontsJS.js"></script>
<script>

    let month = {
        '01': 'Janeiro',
        '02': 'Fevereiro',
        '03': 'Março',
        '04': 'Abril',
        '05': 'Maio',
        '06': 'Junho',
        '07': 'Julho',
        '08': 'Agosto',
        '09': 'Setembro',
        '10': 'Outubro',
        '11': 'Novembro',
        '12': 'Dezembro'
    }

    var date = new Date();
    var dd = String(date.getDate()).padStart(2, '0');
    var mm = String(date.getMonth() + 1).padStart(2, '0'); 
    var yyyy = date.getFullYear();
    date = mm + '/' + dd + '/' + yyyy;
    //var player = GetPlayer();
    //var name = player.GetVar("uName"); /* Replace uName with the Storyline variable holding the user's name */
    var doc = new jsPDF({
    orientation: 'landscape' /* Remove this line of code if your certificate is in portrait orientation */
    })

    if('<%= certificateInfo[0].jcv_course_brand %>' == 'Felps Professional'){

        try {
            // Fontes customizadas
            doc.addFileToVFS("FelpsProfessional.ttf", fontFelpsProfessional);
            doc.addFileToVFS("FelpsProfessionalSemiBold.ttf", FelpsProfessionalSemiBold);
            doc.addFileToVFS("FelpsMen.ttf", fontFelpsMen);

            // Adicionando as fontes
            doc.addFont("FelpsProfessional.ttf", "FelpsProfessional", "Normal");
            doc.addFont("FelpsProfessionalSemiBold.ttf", "FelpsProfessionalSemiBold", "Bold");
            doc.addFont("FelpsMen.ttf", "FelpsMen", "Normal");

            var img = new Image;
            img.onload = function() {
            doc.addImage(this, 0, 0, 297, 210);
            
            doc.setTextColor(29, 28, 29); /* Change the RGB text color by changing the numbers between the parentheses */
            
            //doc.setFont('GraphikRegular_0', 'bold');
            doc.setFont("FelpsProfessionalSemiBold","Bold");
            doc.setFontSize(32); /* Set the font size by changing the number between the parentheses */
            doc.text('<%= dataUserInfo[0].jcv_users_name %>', (doc.internal.pageSize.width / 2 +15), 90, null, null, 'center'); /* This tells the PDF to add text to the page. The first argument is the text to add (in this case a JavaScript variable), the second is the x-coordinate (in this case we’re using a function to find a middle point for the document), the third is the y-coordinate (in millimeters), the fourth and fifth arguments are null, and the sixth argument says that the text should be centered */

            /* ***** */
            let textInsert = '<%= certificateInfo[0].jcv_course_text %>';

            let dateCourseConfig = '<%= certificateInfo[0].jcv_course_initial_date.split("T")[0].split("-")[2] %> de '+month['<%= certificateInfo[0].jcv_course_initial_date.split("T")[0].split("-")[1] %>']+' de <%= certificateInfo[0].jcv_course_initial_date.split("T")[0].split("-")[0] %>'

            //Iserindo as informações
            textInsert = textInsert.replace('${courseName}','<%= certificateInfo[0].jcv_course_name %>')
            textInsert = textInsert.replace('${managerCourse}','<%= getAllInstructors %>')
            textInsert = textInsert.replace('${brandName}','<%= certificateInfo[0].jcv_course_brand %>')
            textInsert = textInsert.replace('${dateCourse}', dateCourseConfig)
            textInsert = textInsert.replace('${hoursCourse}','<%= certificateInfo[0].jcv_course_total_hours %>')

            //console.log(textInsert)

            let getArr = doc.splitTextToSize(textInsert, 100);
            var width = doc.getTextWidth(getArr);
            //console.log(width);
            /* ***** */
            doc.setFont("FelpsProfessional","Normal");
            doc.setFontSize(19);
            doc.text(getArr, ((doc.internal.pageSize.width / 2) + 15), 110, {maxWidth: 160, align: "center"}); /* See above for the full explanation of the doc.text() function */
            /* ***** */

            /* ASSINATURA */
            let imageAss = '<%= certificateInfo[0].jcv_course_signature %>';
            doc.addImage(imageAss, 'jpeg', (doc.internal.pageSize.width - 105), (doc.internal.pageSize.height - 59), 60, 20)// width, heigth, imgWidth, imgGeigth

            doc.save("Certificado.pdf"); /* Swap out ‘Certificate’ with what you want your certificate to be named */
            };
            img.crossOrigin = "";
            img.src = "/certificates/base-felps.png"; /* Change the file name between these quotes to the file that you exported as the base certificate */
        } catch (error) {
            alert('Houve um erro ao baixar seu certificado. Entre em contato com o admistrador')
        }

        
    }else{
        try {
            // Fontes customizadas
            doc.addFileToVFS("FelpsProfessional.ttf", fontFelpsProfessional);
            doc.addFileToVFS("FelpsProfessionalSemiBold.ttf", FelpsProfessionalSemiBold);
            doc.addFileToVFS("FelpsMen.ttf", fontFelpsMen);

            // Adicionando as fontes
            doc.addFont("FelpsProfessional.ttf", "FelpsProfessional", "Normal");
            doc.addFont("FelpsProfessionalSemiBold.ttf", "FelpsProfessionalSemiBold", "Bold");
            doc.addFont("FelpsMen.ttf", "FelpsMen", "Normal");

            var img = new Image;
            img.onload = function() {
            doc.addImage(this, 0, 0, 297, 210);
            
            doc.setTextColor(29, 28, 29); /* Change the RGB text color by changing the numbers between the parentheses */
            
            //doc.setFont('GraphikRegular_0', 'bold');
            doc.setFont("FelpsMen","Normal");
            doc.setFontSize(34); /* Set the font size by changing the number between the parentheses */
            doc.text('<%= dataUserInfo[0].jcv_users_name %>', (doc.internal.pageSize.width / 2 ), 80, null, null, 'center'); /* This tells the PDF to add text to the page. The first argument is the text to add (in this case a JavaScript variable), the second is the x-coordinate (in this case we’re using a function to find a middle point for the document), the third is the y-coordinate (in millimeters), the fourth and fifth arguments are null, and the sixth argument says that the text should be centered */

            /* ***** */
            let textInsert = '<%= certificateInfo[0].jcv_course_text %>';

            let dateCourseConfig = '<%= certificateInfo[0].jcv_course_initial_date.split("T")[0].split("-")[2] %> de '+month['<%= certificateInfo[0].jcv_course_initial_date.split("T")[0].split("-")[1] %>']+' de <%= certificateInfo[0].jcv_course_initial_date.split("T")[0].split("-")[0] %>'

            //Iserindo as informações
            textInsert = textInsert.replace('${courseName}','<%= certificateInfo[0].jcv_course_name %>')
            textInsert = textInsert.replace('${managerCourse}','<%= getAllInstructors %>')
            textInsert = textInsert.replace('${brandName}','<%= certificateInfo[0].jcv_course_brand %>')
            textInsert = textInsert.replace('${dateCourse}', dateCourseConfig)
            textInsert = textInsert.replace('${hoursCourse}','<%= certificateInfo[0].jcv_course_total_hours %>')

            //console.log(textInsert)

            let getArr = doc.splitTextToSize(textInsert, 100);
            var width = doc.getTextWidth(getArr);
            //console.log(width);
            /* ***** */
            doc.setFont("FelpsMen","Normal");
            doc.setFontSize(19);
            doc.text(getArr, ((doc.internal.pageSize.width / 2) ), 100, {maxWidth: 160, align: "center"}); /* See above for the full explanation of the doc.text() function */
            /* ***** */

            /* ASSINATURA */
            let imageAss = '<%= certificateInfo[0].jcv_course_signature %>';
            doc.addImage(imageAss, 'jpeg', (doc.internal.pageSize.width / 2 - 15), (doc.internal.pageSize.height - 76), 70, 30)// width, heigth, imgWidth, imgGeigth
            doc.save("Certificado.pdf"); /* Swap out ‘Certificate’ with what you want your certificate to be named */
            };
            img.crossOrigin = "";
            img.src = "/certificates/base-felps-men.png"; /* Change the file name between these quotes to the file that you exported as the base certificate */
        } catch (error) {
            alert('Houve um erro ao baixar seu certificado. Entre em contato com o admistrador')
        }
    }
</script>
